{
	"$schema": "https://biomejs.dev/schemas/1.3.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"noBannedTypes": "off"
			}
		}
	},
	"formatter": {
		"indentStyle": "space"
	},
	"vcs": {
		"root": "../..",
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	}
}
