{
  "name": "@r4ai/create-project",
  "version": "0.2.0",
  "description": "A CLI to create a new project with some templates",
  "author": "rai",
  "keywords": ["cli", "create", "project", "template"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r4ai/templates.git"
  },
  "bugs": {
    "url": "https://github.com/r4ai/templates/issues"
  },
  "type": "module",
  "files": [
    "src",
    "dist",
    "!**/*.test.ts",
    "!**/*.spec.ts",
    "!**/*.tsbuildinfo",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "create-project": "./dist/create-project.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "build": "bun run ./build.ts",
    "prepublishOnly": "bun run build",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "check:ci": "biome ci .",
    "release": "bun run vitest run && bun run build && bun run changeset publish"
  },
  "peerDependencies": {},
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "giget": "^1.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@changesets/cli": "2.27.7",
    "@types/bun": "1.1.8",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "bun-banner-plugin": "0.1.2",
    "dedent": "1.5.3",
    "rimraf": "5.0.10",
    "typescript": "5.5.4",
    "vitest": "1.6.0"
  },
  "homepage": "https://github.com/r4ai/templates#readme",
  "directories": {
    "test": "tests"
  }
}
